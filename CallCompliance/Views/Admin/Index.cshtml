@using Newtonsoft.Json

@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <form>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">Admissions Daily Dial Cap</label>
            <div class="col-sm-1">
                <input id="1" type="text" class="form-control" data-bind="value: ParmValue1" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">Admissions Daily Exceptions Dial Cap</label>
            <div class="col-sm-1">
                <input id="3" type="text" class="form-control" data-bind="value: ParmValue2" maxlength="2">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">Other Daily Dial Cap</label>
            <div class="col-sm-1">
                <input id="2" type="text" class="form-control" data-bind="value: ParmValue3" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">Other Daily Exceptions Cap</label>
            <div class="col-sm-1">
                <input id="4" type="text" class="form-control" data-bind="value: ParmValue4" maxlength="2">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 0 Cap</label>
            <div class="col-sm-1">
                <input id="13" type="text" class="form-control" data-bind="value: ParmValue5" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 0 Exception Cap</label>
            <div class="col-sm-1">
                <input id="14" type="text" class="form-control" data-bind="value: ParmValue6" maxlength="2">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 1 Cap</label>
            <div class="col-sm-1">
                <input id="15" type="text" class="form-control" data-bind="value: ParmValue7" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 1 Exception Cap</label>
            <div class="col-sm-1">
                <input id="16" type="text" class="form-control" data-bind="value: ParmValue8" maxlength="2">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 2 Cap</label>
            <div class="col-sm-1">
                <input id="19" type="text" class="form-control" data-bind="value: ParmValue9" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">New Lead Day 2 Exception Cap</label>
            <div class="col-sm-1">
                <input id="20" type="text" class="form-control" data-bind="value: ParmValue10" maxlength="2">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-sm-5 text-sm-right col-form-label">Not Engaged Cap</label>
            <div class="col-sm-1">
                <input id="21" type="text" class="form-control" data-bind="value: ParmValue11" maxlength="2">
            </div>
            <label class="col-sm-5 text-sm-right col-form-label">Not Enrolled Cap</label>
            <div class="col-sm-1">
                <input id="22" type="text" class="form-control" data-bind="value: ParmValue12" maxlength="2">
            </div>
        </div>
        
        <div class="form-inline row">
            <div class="align-middle">
                <div class="col-sm-2">
                    <button type="submit" class="btn btn-primary" data-bind="click: Uma.CoolDown.doSaveCallCaps">Save</button>
                </div>
                <div class="col-sm-2">
                    <button type="submit" class="btn btn-primary" data-bind="click: Uma.CoolDown.doReset">Reset</button>
                </div>
            </div>
        </div>

    </form>
</div>

<div class="dropdown">
    <button class="btn btn-primary dropdown-toggle dropup" type="button" data-toggle="dropdown">
        Choose List
        <span class="caret"></span>
    </button>

    <ul class="dropdown-menu">
        <li><a href="#">DNC List</a></li>
        <li><a href="#">Exceptions</a></li>
        <li><a href="#">Reports</a></li>
        <li class="divider"></li>
        <li><a href="#">Help!</a></li>
    </ul>
    
</div>
<br/>
<br />
<br />
<div class="list-inline col-sm-12">
    <div class="col-sm-4">
        <a href="#" class="list-group-item active">
            Admissions
        </a>
        <a href="#" class="list-group-item list-group-item-action">Completer</a>
        <a href="#" class="list-group-item list-group-item-action">Dismissal</a>
        <a href="#" class="list-group-item list-group-item-action">Invalid Lead</a>
        <a href="#" class="list-group-item list-group-item-action disabled">New Lead</a>
        <a href="#" class="list-group-item list-group-item-action">Post Start Cancel</a>
        <a href="#" class="list-group-item list-group-item-action">Post Start Cancel - NS</a>
        <a href="#" class="list-group-item list-group-item-action">Pre-Start Cancel</a>
        <a href="#" class="list-group-item list-group-item-action disabled">Reverse ReEntry</a>
        <a href="#" class="list-group-item list-group-item-action disabled">Withdrawal</a>
    </div>
    <div class="col-sm-4">
        <a href="#" class="list-group-item active">
            Other
        </a>
        <a href="#" class="list-group-item list-group-item-action">Completer</a>
        <a href="#" class="list-group-item list-group-item-action">Dismissal</a>
        <a href="#" class="list-group-item list-group-item-action"></a>
        <a href="#" class="list-group-item list-group-item-action disabled"></a>
        <a href="#" class="list-group-item list-group-item-action"></a>
        <a href="#" class="list-group-item list-group-item-action"></a>
        <a href="#" class="list-group-item list-group-item-action"></a>
        <a href="#" class="list-group-item list-group-item-action disabled"></a>
        <a href="#" class="list-group-item list-group-item-action disabled"></a>
    </div>
</div>



@section scripts {
    
<script src="~/ViewScripts/Admin/Admin_ViewModel.js"></script>
    <script type="text/javascript">
        $(function () {

            Uma.Admin.serverModel = @Html.Raw(JsonConvert.SerializeObject(Model));
            Uma.Admin.init();

            $("#menu-admin").addClass("active");
            ko.applyBindings();
        })
    </script>
}



