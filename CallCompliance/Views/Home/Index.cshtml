
@using Newtonsoft.Json

@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br/>
<div class="jumbotron">
    <h3>Welcome to the Call Compliance Portal, @MyHelpers.UserFullName()!</h3>
</div>
    @section scripts {

        <script>
            $(function () {
                Uma.Login.serverModel = @Html.Raw(JsonConvert.SerializeObject(Model));
                Uma.Login.init();

                $("#menu-home").addClass("active");
                ko.applyBindings();
            })
        </script>
    }

