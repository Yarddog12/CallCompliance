@model CallCompliance.ViewModels.UnblockViewModel

@{
    ViewBag.Title = "Unblock";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<form>
    <div class="form-group">
        @Html.LabelFor(m => m.PhoneNumber)
        @Html.TextBoxFor(m => m.PhoneNumber, new { @class="form-control"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.PhoneNumber)
        @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.ReasonOverrides)
        @Html.TextBoxFor(m => m.PhoneNumber, new SelectList (Model.ReasonOverrides, "Id", "Name");
    </div>
</form>

@section scripts {
    <script type="text/javascript">
        $(function () {
            $("#menu-unblock").addClass("active");
            ko.applyBindings();
        })
    </script>
}